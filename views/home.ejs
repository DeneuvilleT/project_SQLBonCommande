<h1>
   <%=h1%>
</h1>


<table class="standard-table">

   <caption>
      <%=title%>
   </caption>
   <thead>
      <tr>
         <th>
            <%=col1%>
         </th>
         <th>
            <%=col2%>
         </th>
         <th>
            <%=col3%>
         </th>
         <th>
            <%=status%>
         </th>
      </tr>
   </thead>
   <tbody>
      <% data.forEach((ele)=>{ %>
         <tr>
            <td>
               <a href="">
                  <%= ele.orderNumber %>
               </a>
            </td>

            <td>
               <%= ele.orderDate %>
            </td>

            <td>
               <%= ele.shippedDate %>
            </td>

            <td>
               <%=ele.status%>
            </td>
         </tr>
         <%})%>
   </tbody>
</table>